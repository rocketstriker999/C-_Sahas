{
  "name": "electron_sahas",
  "version": "1.0.2",
  "description": "This is Windows app For Sahas",
  "main": "app.js",
  "scripts": {
    "start": "npm install && electron .",
    "build": "npm install && electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketstriker999/electron_Sahas.git"
  },
  "keywords": [
    "Sahas"
  ],
  "build": {
    "productName": "Sahas",
    "appId": "com.hammerbyte.sahas",
    "publish": [
      {
        "provider": "generic",
        "url": "https://sahasinstitute.com/assets/win_updates"
      }
    ],
    "directories": {
      "output": "builds/"
    },
    "win": {
      "icon": "./assets/img/sahas.png"
    },
    "linux": {
      "icon": "./assets/img/sahas.png",
      "category": "Utility",
      "target": "deb"
    }
  },
  "author": "HammerByte",
  "license": "ISC",
  "devDependencies": {
    "electron": "^27.1.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "electron-updater": "^6.1.7"
  }
}